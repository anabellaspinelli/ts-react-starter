{
  "compilerOptions": {
    "allowJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "noImplicitAny": true,
    "jsx": "react-jsx",
    "lib": [
      "dom",
      "es2017",
      "webworker"
    ],
    "module": "esnext",
    "moduleResolution": "node",
    "noEmit": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": false,
    "noUnusedParameters": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "target": "es5",
    "typeRoots": [
      "./node_modules/@types",
      "./types"
    ],
    "types": [
      "jest"
    ],
    "sourceRoot": "/",
    "sourceMap": true,
    "inlineSources": true,
    "baseUrl": ".",
    "allowSyntheticDefaultImports": true,
    "strict": true
  },
  "include": [
    "src"
  ]
}
